#include <stdio.h>

int main()
{
    int item[20];
    printf("Enter the number.");
    scanf("%d %d %d %d %d %d %d %d %d %d", &item[0], &item[1], &item[2], &item[3], &item[4], &item[5], &item[6], &item[7], &item[8], &item[9]);

    for (int i = 1; i < 11; i++)
    {
        printf("%2d %2d %2d %2d %2d %2d %2d %2d %2d %2d \n", i*item[0], i*item[1], i*item[2], i*item[3] , i*item[4], i*item[5], i*item[6], i*item[7] , i*item[8], i*item[9]);
    }
    return 0;
}
